{
  "tasks": {
    "dev": "deno fmt --watch & deno run -A --node-modules-dir=auto npm:vite",
    "build": "deno run -A --node-modules-dir=auto npm:vite build",
    "server:start": "deno run -A --node-modules-dir --watch ./server/main.ts",
    "serve": "deno task build && deno task server:start"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.4",
    "@std/assert": "jsr:@std/assert@^1.0.11",
    "@tanstack/react-query": "npm:@tanstack/react-query@^5.69.0",
    "@types/react": "npm:@types/react@^18.3.19",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.4",
    "chart.js": "npm:chart.js@^4.4.8",
    "primeflex": "npm:primeflex@^4.0.0",
    "primeicons": "npm:primeicons@^7.0.0",
    "primereact": "npm:primereact@^10.9.3",
    "quill": "npm:quill@^2.0.3",
    "react": "https://esm.sh/react@18.3.1",
    "react-dom": "https://esm.sh/react-dom@18.3.1",
    "react-router-dom": "npm:react-router-dom@^6.30.0",
    "sass": "npm:sass@^1.86.0",
    "vite": "npm:vite@^5.4.14"
  },
  "compilerOptions": {
    "types": [
      "react",
      "react-dom",
      "@types/react"
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "https://esm.sh/react@18.3.1"
  },
  "nodeModulesDir": "auto",
  "moduleResolution": "node"
}
